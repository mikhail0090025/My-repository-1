define(["OK/utils/dom"],function(b){function a(){}a.prototype.activate=function(d){var c=this;this.showLinkElement=b.firstByClass(d,"js-showLink");this.showLinkElementHandler=function(){c.showLinkElement.style.display="none";var e=b.firstByClass(d,"js-showLinkInput");e.style.display="block";e.select();};this.showLinkElement.addEventListener("click",this.showLinkElementHandler);};a.prototype.deactivate=function(){this.showLinkElement.removeEventListener("click",this.showLinkElementHandler);};return a;});