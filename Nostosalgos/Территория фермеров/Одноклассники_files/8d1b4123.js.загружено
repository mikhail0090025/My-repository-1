define(function(){return{activate:function(container){var scripts=container.getElementsByTagName("script");for(var i=0;i<scripts.length;i++){var text=scripts[i].innerHTML;if(text){eval(text.replace(/(^[^<]*<!--)|(-->[^>]*$)/g,""));}}var activateAttr=container.getAttribute("ac");if(activateAttr){eval(activateAttr);}},deactivate:function(container){var deactivateAttr=container.getAttribute("deac");if(deactivateAttr){eval(deactivateAttr);}}};});