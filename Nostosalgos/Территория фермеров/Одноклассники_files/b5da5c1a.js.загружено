define(["OK/HookModel"],function(a){var b;return{activate:function(d){var f=d.getAttribute("data-loader-id"),c=a.extractHookId(f);b=a.hookActivated(c);b.then(e);function e(){setTimeout(function(){var h=document.createEvent("MouseEvents"),g=d.parentNode.getElementsByClassName("js-show-more")[0];h.initEvent("click",false,true);g.dispatchEvent(h);});}},deactivate:function(d){var e=d.getAttribute("data-loader-id"),c=a.extractHookId(e);if(b){a.removeObserver(b,c);}}};});