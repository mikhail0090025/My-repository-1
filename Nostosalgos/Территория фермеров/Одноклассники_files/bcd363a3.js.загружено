define(function(b){var a=b("OK/utils/utils"),d=b("OK/utils/dom"),c="data-url";return{activate:function(f){var g=function(){a.ajax({url:f.getAttribute(c)}).done(function(j){var h=d.firstByClass(f,"js-updater_cnt");if(j&&h){f.classList.remove("__loading");h.innerHTML=j;OK.loader.execRequire("OK/capture",function(l){d.forEach(h.childNodes,function(m){l.activate(m);});});var i=d.firstByClass(h,"js-data-holder"),k=i?i.getAttribute("data-new-url"):null;if(k){f.setAttribute(c,k);}}});};g();var e=d.firstByClass(f,"js-updater_refresh");if(e){e.addEventListener("click",g);}}};});